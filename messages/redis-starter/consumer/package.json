{
  "name": "redis-consumer",
  "version": "0.0.1",
  "description": "Redis Consumer",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "dev": "kill-port 4001 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@hieudoanm/express": "0.0.1",
    "@hieudoanm/pino": "0.0.1",
    "@hieudoanm/redis": "0.0.1"
  },
  "devDependencies": {
    "@types/ioredis": "4.28.10",
    "@types/jest": "29.1.2",
    "@types/node": "18.6.3",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.25.0",
    "jest": "29.1.2",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
