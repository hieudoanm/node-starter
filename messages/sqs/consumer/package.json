{
  "name": "@sqs/consumer",
  "version": "0.0.1",
  "description": "AWS SQS Consumer",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-message-brokers.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 18",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "serve": "kill-port 4001 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/apis/swagger.json > ./docs/apis/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@hieudoanm/dotenv": "0.0.1",
    "@hieudoanm/express": "0.0.1",
    "@hieudoanm/pino": "0.0.1",
    "aws-sdk": "2.1274.0"
  },
  "devDependencies": {
    "@types/express": "4.17.15",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.13",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "swagger2-to-postmanv2": "2.1.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
