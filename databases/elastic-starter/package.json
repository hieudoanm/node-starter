{
  "name": "@starter/elasticsearch",
  "version": "0.0.1",
  "description": "ElasticSearch Starter",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@hieudoanm/dotenv": "0.0.1",
    "@hieudoanm/elasticsearch": "0.0.1",
    "@hieudoanm/express": "0.0.1",
    "@hieudoanm/pino": "0.0.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/csurf": "1.11.2",
    "@types/elasticsearch": "5.0.40",
    "@types/express": "4.17.14",
    "@types/http-errors": "1.8.2",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.5",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@webpack-cli/generators": "2.5.0",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "jest": "29.2.2",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "supertest": "6.3.1",
    "swagger2-to-postmanv2": "2.1.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
