{
  "name": "@starter/express",
  "version": "1.0.0",
  "description": "Starter - Express",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch",
    "prisma:generate": "prisma format && prisma generate"
  },
  "dependencies": {
    "@hieudoanm/axios": "0.0.1",
    "@hieudoanm/dotenv": "0.0.1",
    "@hieudoanm/express": "0.0.1",
    "@hieudoanm/jwt": "0.0.1",
    "@hieudoanm/mongodb": "0.0.1",
    "@hieudoanm/pino": "0.0.1",
    "@hieudoanm/redis": "0.0.1",
    "@prisma/client": "4.7.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/csurf": "1.11.2",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.10",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@webpack-cli/generators": "3.0.0",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.20",
    "prisma": "4.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.3.2",
    "swagger2-to-postmanv2": "2.1.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}
