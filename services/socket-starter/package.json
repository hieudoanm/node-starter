{
  "name": "@starter/socket",
  "version": "0.0.1",
  "description": "Socket Starter",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "dev": "kill-port 8080 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger.json > ./docs/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@hieudoanm/dotenv": "0.0.1",
    "@hieudoanm/express": "0.0.1",
    "@hieudoanm/pino": "0.0.1",
    "socket.io": "4.5.3",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/csurf": "1.11.2",
    "@types/express": "4.17.14",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "@types/webpack-node-externals": "2.5.3",
    "@types/ws": "8.5.3",
    "@webpack-cli/generators": "3.0.0",
    "concurrently": "7.5.0",
    "eslint": "8.28.0",
    "jest": "29.3.1",
    "kill-port": "2.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0",
    "webpack-node-externals": "3.0.0"
  }
}
