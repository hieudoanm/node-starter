{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Express Starter",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/express-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "build:hello": "yarn workspace hello-starter build",
    "build:key-cloak": "yarn workspace key-cloak-starter build",
    "build:mongo": "yarn workspace mongo-starter build",
    "build": "yarn build:hello && yarn build:key-cloak && yarn build:mongo",
    "dev:hello": "yarn workspace hello-starter dev",
    "dev:key-cloak": "yarn workspace key-cloak-starter dev",
    "dev:mongo": "yarn workspace mongo-starter dev",
    "dev": "yarn dev:hello && yarn dev:key-cloak && yarn dev:mongo",
    "lint:hello": "yarn workspace hello-starter lint",
    "lint:key-cloak": "yarn workspace key-cloak-starter lint",
    "lint:mongo": "yarn workspace mongo-starter lint",
    "lint": "yarn lint:hello && yarn lint:key-cloak && yarn lint:mongo",
    "test:hello": "yarn workspace hello-starter test",
    "test:key-cloak": "yarn workspace key-cloak-starter test",
    "test:mongo": "yarn workspace mongo-starter test",
    "test": "yarn test:hello && yarn test:key-cloak && yarn test:mongo",
    "graph": "nx graph"
  },
  "devDependencies": {
    "concurrently": "7.3.0",
    "husky": "8.0.1",
    "nx": "14.5.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "@nrwl/nx-cloud": "latest"
  },
  "workspaces": [
    "services/*"
  ]
}
