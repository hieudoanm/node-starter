{
  "name": "node-starter",
  "version": "0.0.1",
  "description": "Express Starter",
  "main": "dist/server.js",
  "repository": "git@github.com:hieudoanm/node-starter.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "build:apollo": "yarn workspace @starter/apollo build",
    "build:elasticsearch": "yarn workspace @starter/elasticsearch build",
    "build:express": "yarn workspace @starter/express build",
    "build:hedera": "yarn workspace @starter/hedera build",
    "build:postgres": "yarn workspace @starter/postgres build",
    "build:socket": "yarn workspace @starter/socket build",
    "build:solana": "yarn workspace @starter/solana build",
    "dev:apollo": "yarn workspace @starter/apollo dev",
    "dev:elasticsearch": "yarn workspace @starter/elasticsearch dev",
    "dev:express": "yarn workspace @starter/express dev",
    "dev:hedera": "yarn workspace @starter/hedera dev",
    "dev:postgres": "yarn workspace @starter/postgres dev",
    "dev:socket": "yarn workspace @starter/socket dev",
    "dev:solana": "yarn workspace @starter/solana dev",
    "lint:apollo": "yarn workspace @starter/apollo lint",
    "lint:elasticsearch": "yarn workspace @starter/elasticsearch lint",
    "lint:express": "yarn workspace @starter/express lint",
    "lint:hedera": "yarn workspace @starter/hedera lint",
    "lint:postgres": "yarn workspace @starter/postgres lint",
    "lint:socket": "yarn workspace @starter/socket lint",
    "lint:solana": "yarn workspace @starter/solana lint",
    "test:apollo": "yarn workspace @starter/apollo test",
    "test:elasticsearch": "yarn workspace @starter/elasticsearch test",
    "test:express": "yarn workspace @starter/express test",
    "test:hedera": "yarn workspace @starter/hedera test",
    "test:postgres": "yarn workspace @starter/postgres test",
    "test:socket": "yarn workspace @starter/socket test",
    "test:solana": "yarn workspace @starter/solana test",
    "graph": "nx graph"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "concurrently": "7.5.0",
    "husky": "8.0.2",
    "nx": "15.0.11",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3"
  },
  "workspaces": [
    "contracts/*",
    "databases/*",
    "packages/*",
    "serverless/*",
    "services/*"
  ]
}
